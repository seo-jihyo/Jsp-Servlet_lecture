	<servlet>
	<servlet-name>ServletDate</servlet-name>
	<servlet-class>com.kosa.date.ServletDate</servlet-class>
	</servlet>
	
	<servlet-mapping>
	<servlet-name>ServletDate</servlet-name>
	<url-pattern></url-pattern>
	</servlet-mapping>
	
	
